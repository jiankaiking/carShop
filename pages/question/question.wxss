.quistionBox{ width: 100%; padding: 0 30rpx; box-sizing: border-box;}
.quistTitle{  font-size: 36rpx; color: #333;font-weight: 600; padding: 40rpx 0;}
.quistNav{ width: 100%;}
.quistItem{ width: 100%; overflow: hidden;}
.quistItemTitle{ width: 100%; border-bottom: 1px solid #E3E3E3; align-items: center; font-size: 32rpx; color: #333; display: flex; justify-content: space-between; padding: 20rpx 0;}
.quistItemTitle image{ width: 27rpx; height: 15rpx; transition: transform .3s; }
.quistMsg{display: none; width: 100%; padding: 15rpx 30rpx;box-sizing: border-box; font-size: 28rpx; color: #666666; opacity: 0;transition:all .3s;}
.opc{ opacity: 1; display: block}
page{ width: 100%; height: 100%;background:#fff;}
.height{height: 100%; }
.quiTitletext{ text-overflow: ellipsis; overflow: hidden; max-width: 600rpx; white-space: nowrap;}
.quistItemTitle image.transImg{ transform: rotate(-180deg);}