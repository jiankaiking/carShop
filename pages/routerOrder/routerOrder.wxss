page {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}

.routerordernav {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  margin-top: 30rpx;
}

.navItem {
  width: 100%;
  background: #fff;
  padding: 0 30rpx;
  box-sizing: border-box;
  margin-bottom: 30rpx;
}

.itemmsg {
  width: 100%;
  display: flex;
  justify-content: flex-start;
  border-bottom: 1px solid #e3e3e3;
}

.itemImg, .itemImg image {
  width: 80rpx;
  height: 80rpx;
}

.itemImg {
  padding: 30rpx 0;
  margin-right: 16rpx;
}

.itemtext {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 30rpx 0;
}

.itemName {
  color: #333;
  font-size: 32rpx;
}

.itemTime {
  font-size: 24rpx;
  color: #999;
}

.itemStatus {
  display: flex;
  justify-content: space-between;
  padding: 30rpx 0;
}

.itemStatus text:first-child {
  color: #666;
  font-size: 28rpx;
}

.itemStatus text:last-child {
  color: #333;
  font-size: 28rpx;
}

.orderNone image {
  width: 250rpx;
  height: 250rpx;
  margin: 156rpx 0 50rpx;
}

.orderNone {
  color: #999;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
}
